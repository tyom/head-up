{
  "name": "head-up",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-demo": "vue-tsc --noEmit && vite build -c demo/vite.config.js",
    "serve": "vite preview -c demo/vite.config.js",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts,.vue --ignore-path .gitignore"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/head-up.umd.js",
  "module": "./dist/head-up.es.js",
  "exports": {
    ".": {
      "import": "./dist/head-up.umd.js",
      "require": "./dist/head-up.es.js"
    }
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.2.2",
    "axios": "^0.22.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^3.1.29",
    "parse-duration": "^1.0.0",
    "tinykeys": "^1.2.0",
    "vue": "^3.2.19",
    "vue-axios": "^3.3.7",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@iconify/vue": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.20",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.19.1",
    "postcss": "^8.3.9",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3",
    "vite": "^2.6.5",
    "vue-tsc": "^0.28.0"
  }
}
