{
  "name": "head-up",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev-examples": "vite -c examples/vite.config.js",
    "build": "vue-tsc --noEmit && vite build",
    "build-examples": "vue-tsc --noEmit && vite build -c examples/vite.config.js",
    "serve": "vite preview -c examples/vite.config.js",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts,.vue --ignore-path .gitignore"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/head-up.umd.js",
  "module": "./dist/head-up.es.js",
  "exports": {
    ".": {
      "import": "./dist/head-up.umd.js",
      "require": "./dist/head-up.es.js"
    }
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.16.0",
    "postcss": "^8.3.6",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2"
  }
}
