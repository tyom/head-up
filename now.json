{
  "version": 2,
  "name": "head-up-example",
  "alias": "head-up-example",
  "builds": [
    { "src":  "packages/example/dist/**/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/packages/example/dist/$1" }
  ],
  "env": {
    "VUE_APP_TMDB_API_KEY": "@head-up-tmdb-api-key"
  }
}
